{
    "name": "Search On ...",
    "version": "0.1",
    "manifest_version": 2,
    "description": "A Chromium Extension to run search on other search engines",
    "icons": {
        "16": "icons/gtk_find_and_replace16.png",
        "32": "icons/gtk_find_and_replace32.png",
        "48": "icons/gtk_find_and_replace48.png",
        "128": "icons/gtk_find_and_replace128.png"
    },
    "permissions": [
        "tabs"
      ],
    "browser_action": {
        "default_popup": "popup/popup.html"
    }
}